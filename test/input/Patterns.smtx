
main module patterns {

    $a + b$
    (a: int, b: int) -> int {
        @addint(a, b)
    }

    $print n$
    (n: int) {
        @printint(n);
    }

    $print str$
    (str: string) {
        @printstring(str);
    }

    $println n$
    (n: int) {
        print n;
        print "\n";
    }

    println 5 + 4;

}